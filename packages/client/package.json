    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "vite": "^5.3.1"
  },
  "dependencies": {
    "@apollo/client": "^3.10.7",
    "compression": "^1.7.4",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "react": "^18.2.0",
    "react-cookie": "^7.1.4",
    "react-dom": "^18.2.0",
